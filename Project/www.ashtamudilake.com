<script type="text/javascript">
function emailcheck(str) {
		var at="@"
		var dot="."
		var lat=str.indexOf(at)
		var lstr=str.length
		var ldot=str.indexOf(dot)
		if (str.indexOf(at)==-1){
		   alert("Invalid E-mail ID")
		   return false
		}

		if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr)
		{
		   alert("Invalid E-mail ID")
		   return false
		}

		if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr)
		{
		    alert("Invalid E-mail ID")
		    return false
		}

		 if (str.indexOf(at,(lat+1))!=-1){
		    alert("Invalid E-mail ID")
		    return false
		 }

		 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
		    alert("Invalid E-mail ID")
		    return false
		 }

		 if (str.indexOf(dot,(lat+2))==-1){
		    alert("Invalid E-mail ID")
		    return false
		 }
		
		 if (str.indexOf(" ")!=-1){
		    alert("Invalid E-mail ID")
		    return false
		 }
 	else{
 		 return true					
		 }
	}

function check(){
	
	if(document.form1.name.value=="Name"){
		alert("Please enter your name")
		document.form1.name.focus()
		return false			
	}
	 else if(document.form1.event.value=="Select Required Event"){
		alert("Please choose event")
		document.form1.event.focus()
		return false	
		}
		
		else if(document.form1.mobile.value=="Mobile"){
		alert("Please enter your mobile number")
		document.form1.mobile.focus()
		return false					
	} 
		
	
	else if(document.form1.email.value=="Email"){
		alert("Please enter your email address")
		document.form1.email.focus()
		return false	
	} else if(emailcheck(document.form1.email.value)==false){
		document.form1.email.focus()
		return false	
	} 

	
	else if(document.form1.city.value=="City"){
		alert("Please enter your city")
		document.form1.city.focus()
		return false					
	} 
	
	else{
		document.form1.submit();
		return true
	}
   }
